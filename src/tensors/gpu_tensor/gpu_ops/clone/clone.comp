#version 450
layout(local_size_x = 1) in;

layout(set = 0, binding = 0) buffer Data {
     float[] input_data;
};

layout(set = 0, binding = 1) buffer OutputBuffer {
    float[] output_buffer;
};


void main() {
    uint index = gl_GlobalInvocationID.x;
    output_buffer[index] = input_data[index];
}


